{
    "title": "SRE Overview - Farmamaster",
    "panels": [
        {
            "title": "Global Availability (30d)",
            "type": "stat",
            "query": "sum(rate(http_requests_total{status=~'2..'}[30d])) / sum(rate(http_requests_total[30d]))",
            "thresholds": [
                {
                    "value": 0.9995,
                    "color": "green"
                },
                {
                    "value": 0.99,
                    "color": "red"
                }
            ]
        },
        {
            "title": "create-tenant Latency (p95)",
            "type": "graph",
            "query": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{handler='create-tenant'}[5m])) by (le))",
            "unit": "s"
        },
        {
            "title": "Remaining Error Budget",
            "type": "gauge",
            "value": 480,
            "max": 500
        }
    ]
}